Host github.com
    HostName github.com
    AddKeysToAgent yes
    User git
    IdentityFile ~/.ssh/cern
    IdentitiesOnly yes

Host gitlab.cern.ch
    HostName gitlab.cern.ch
    User git
    IdentitiesOnly yes
    IdentityFile ~/.ssh/cern

Host *.cern.ch !lxtunnel.cern.ch !lxplus.cern.ch !aiadm.cern.ch !lxtunnel !lxplus !aiadm !gitlab.cern.ch
    User chbarnes
    ProxyJump lxtunnel.cern.ch

Host lxtunnel.cern.ch lxplus.cern.ch aiadm.cern.ch lxtunnel lxplus aiadm
    User chbarnes
    # if your client is configured for Kerberos
    GSSAPIDelegateCredentials yes
    GSSAPIAuthentication yes
    # SOCKS5 proxy for web, see below
    DynamicForward 8090   
    ExitOnForwardFailure no

# Some specifics for if logging in as root or user
Match Host *.cern.ch User root
    # Don't forward my personal krb tickets when logging in as root.
    GSSAPIDelegateCredentials no
    ForwardAgent no
    PubkeyAuthentication yes
